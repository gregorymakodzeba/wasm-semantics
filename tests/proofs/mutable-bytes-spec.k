        => end(b"a")
    claim start(substrBytes(replaceAtBytes(padRightBytes(.MutableBytes, 128), 16, b"a"), 16, 17))
        => end(b"a")
    claim start(substrBytes(replaceAtBytes(padRightBytes(.MutableBytes, 128), 31, b"a"), 31, 32))
        => end(b"a")
